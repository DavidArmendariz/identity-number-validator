{
  "name": "identity-number-validator",
  "version": "1.0.0",
  "description": "Package to validate if an identity number is valid or not",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "check-types": "tsc --noEmit",
    "coverage": "jest --clearCache && jest --coverage",
    "lint": "eslint 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidArmendariz/identity-number-validator.git"
  },
  "author": "David Armend√°riz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DavidArmendariz/identity-number-validator/issues"
  },
  "homepage": "https://github.com/DavidArmendariz/identity-number-validator#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-types && yarn lint",
      "pre-push": "yarn coverage && yarn publish"
    }
  },
  "files": ["build/**/*"],
  "keywords": ["ecuador", "cedula", "identity"]
